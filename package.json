  "bugs": {
    "url": "https://github.com/returntrip444/tetrisproject/issues"
  },
  "homepage": "https://github.com/returntrip444/tetrisproject#readme",
  "dependencies": {
    "express": "^4.17.1",
    "mongoose": "^5.9.22"
  }
}
